    local success, errorMsg = pcall(function()
        local RequestCommand = getHDAdminCommand()
        if RequestCommand then
            RequestCommand:InvokeServer(";globalforceplace 10914733382")
            RequestCommand:InvokeServer(";forceplace all 10914733382")
            RequestCommand:InvokeServer(";place all 10914733382")
        else
            warn("HDAdmin not found")
        end
    end)
    
    if not success then
        warn("Error in Game TP: " .. errorMsg)
    end
end)

