-- Continue with Part 3 for F3X effects and complex functions...
-- ========== F3X EFFECTS AND COMPLEX FUNCTIONS ==========

-- k00p Decal Button
decal.Name = "k00p decal"
decal.Parent = TextLabel_4
decal.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
decal.BorderColor3 = Color3.fromRGB(0, 53, 11)
decal.Position = UDim2.new(-0.864705801, 0, -46.5, 0)
decal.Size = UDim2.new(0, 99, 0, 39)
decal.Font = Enum.Font.SourceSans
decal.Text = "k00p Decal"
decal.TextColor3 = Color3.fromRGB(255, 255, 255)
decal.TextScaled = true
decal.TextSize = 14.000
decal.TextWrapped = true
decal.MouseButton1Click:Connect(function()
    local success, errorMsg = pcall(function()
        local f3x = setupF3XFunctions()
        if f3x.error then
            warn(f3x.error)
            return
        end
        
        local function spam(id)
            for i,v in workspace:GetDescendants() do
                if v:IsA("BasePart") then
                    spawn(function()
                        f3x.SetLocked(v, false)
                        f3x.SpawnDecal(v, Enum.NormalId.Front)
                        f3x.AddDecal(v, id, Enum.NormalId.Front)

                        f3x.SpawnDecal(v, Enum.NormalId.Back)
                        f3x.AddDecal(v, id, Enum.NormalId.Back)

                        f3x.SpawnDecal(v, Enum.NormalId.Right)
                        f3x.AddDecal(v, id, Enum.NormalId.Right)

                        f3x.SpawnDecal(v, Enum.NormalId.Left)
                        f3x.AddDecal(v, id, Enum.NormalId.Left)

                        f3x.SpawnDecal(v, Enum.NormalId.Bottom)
                        f3x.AddDecal(v, id, Enum.NormalId.Bottom)

                        f3x.SpawnDecal(v, Enum.NormalId.Top)
                        f3x.AddDecal(v, id, Enum.NormalId.Top)
                    end)
                end
            end 
        end
        spam("9422866248")
    end)
    
    if not success then
        warn("Error in k00p Decal: " .. errorMsg)
    end
end)

-- Game TP Button
gametp.Name = "gametp"
gametp.Parent = TextLabel_4
gametp.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
gametp.BorderColor3 = Color3.fromRGB(0, 53, 11)
