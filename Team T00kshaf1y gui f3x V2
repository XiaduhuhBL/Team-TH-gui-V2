-- Special Functions Category
createCategoryLabel("=== SPECIAL FUNCTIONS ===", ScrollFrame)

local realmBtn = createButton("Create Realm", ScrollFrame)
realmBtn.MouseButton1Click:Connect(function()
	executeHDCommand(";btools me")
	executeHDCommand(";sm Team t00kshaf1y join now!!")
	wait(0.6)
	executeHDCommand(";fogcolor black")
	wait(0.5)
	executeHDCommand(";punish all")
	executeHDCommand(";m Hacked by us")
	wait(0.1)
	executeHDCommand(";sh You are about to go to our realm!!")
	
	-- Destroy all existing parts
	local f3x = setupF3XFunctions()
	if f3x then
		for _, v in ipairs(workspace:GetDescendants()) do
			if v:IsA("BasePart") or v:IsA("UnionOperation") then
				spawn(function()
					f3x.DestroyPart(v)
				end)
			end
		end
	end
	
	wait(1)
	
	-- Create new realm base
	executeHDCommand(";fogcolor black ;time")
	createBaseplate()
	
	-- Additional realm setup
	executeHDCommand(";res all")
	wait(0.3)
	executeHDCommand(";r6 all")
	executeHDCommand(";blur all")
	wait(2.8)
	executeHDCommand(";fog")
	executeHDCommand(";sh This is the realm of team T00kshaf1y!")
	executeHDCommand(";disco")
	executeHDCommand(";n Team t00kshaf1y join now!!")
	wait(2.0)
	executeHDCommand(";alert all Welcome to our team's realm!!")
	executeHDCommand(";fire all")
	executeHDCommand(";dog all")
	wait(1.6)
	executeHDCommand(";title me t00kshaf1y")
	executeHDCommand(";name all t00kshaf1y")
	wait(2.0)
	executeHDCommand(";time 23")
	executeHDCommand(";music 1847661821 ;volume inf  ;savemap ;pitch 1.1")
	wait(5.0)
	executeHDCommand(";char all ttsghdvhfc2")
	executeHDCommand(";freeze all")
	executeHDCommand(";sh hacked by team t00kshaf1y")
	executeHDCommand(";sm This game was destroyed by us!")    
	wait(3.0)
	executeHDCommand(";jail all")
	executeHDCommand(";sh lol")
	executeHDCommand(";loopkill all")
end)

