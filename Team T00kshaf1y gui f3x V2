-- ========== SHARED FUNCTIONS ==========
local function getf3x()
    local player = game.Players.LocalPlayer
    for _, v in ipairs(player.Backpack:GetChildren()) do
        if v:FindFirstChild("SyncAPI") then
            return v
        end
    end
    if player.Character then
        for _, v in ipairs(player.Character:GetChildren()) do
            if v:FindFirstChild("SyncAPI") then
                return v
            end
        end
    end
    return nil
end

local function getHDAdminCommand()
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    local HDAdminClient = ReplicatedStorage:FindFirstChild("HDAdminHDClient")
    if HDAdminClient then
        local Signals = HDAdminClient:FindFirstChild("Signals")
        if Signals then
            return Signals:FindFirstChild("RequestCommandSilent")
        end
    end
    return nil
end

local F3XFunctions = {}
local function setupF3XFunctions()
    if F3XFunctions.remote then return F3XFunctions end
    
    local f3x = getf3x()
    if not f3x then
        F3XFunctions.error = "F3X tool not found"
        return F3XFunctions
    end
    
    local remote = f3x.SyncAPI.ServerEndpoint
    
    local function _(args)
        return remote:InvokeServer(unpack(args))
    end
    
    F3XFunctions.remote = remote
    F3XFunctions._ = _
    
    F3XFunctions.SetCollision = function(part, boolean)
        local args = {
            [1] = "SyncCollision",
            [2] = {
                [1] = {
                    ["Part"] = part,
                    ["CanCollide"] = boolean
                }
            }
        }
        _(args)
    end
    
    F3XFunctions.SetAnchor = function(boolean, part)
        local args = {
            [1] = "SyncAnchor",
            [2] = {
                [1] = {
                    ["Part"] = part,
                    ["Anchored"] = boolean
                }
            }
        }
        _(args)
    end
    
    F3XFunctions.CreatePart = function(cf, parent)
        local args = {
            [1] = "CreatePart",
            [2] = "Normal",
            [3] = cf,
            [4] = parent or workspace
        }
        return _(args)
    end
    
    F3XFunctions.DestroyPart = function(part)
        local args = {
            [1] = "Remove",
            [2] = {
                [1] = part
            }
        }
        _(args)
    end
    
    F3XFunctions.Resize = function(part, size, cf)
        local args = {
            [1] = "SyncResize",
            [2] = {
                [1] = {
                    ["Part"] = part,
                    ["CFrame"] = cf,
                    ["Size"] = size
                }
            }
        }
        _(args)
    end
    
    F3XFunctions.AddMesh = function(part)
        local args = {
            [1] = "CreateMeshes",
            [2] = {
                [1] = {
                    ["Part"] = part
                }
            }
        }
        _(args)
    end
    
    F3XFunctions.SetMesh = function(part, meshid)
        local args = {
            [1] = "SyncMesh",
            [2] = {
                [1] = {
                    ["Part"] = part,
                    ["MeshId"] = "rbxassetid://" .. meshid
                }
            }
        }
        _(args)
    end
    
    F3XFunctions.SetTexture = function(part, texid)
        local args = {
            [1] = "SyncMesh",
            [2] = {
                [1] = {
                    ["Part"] = part,
                    ["TextureId"] = "rbxassetid://" .. texid
                }
            }
        }
        _(args)
    end
    
    F3XFunctions.SetName = function(part, stringg)
        local args = {
            [1] = "SetName",
            [2] = {
                [1] = part
            },
            [3] = stringg
        }
        _(args)
    end
    
    F3XFunctions.MeshResize = function(part, size)
        local args = {
            [1] = "SyncMesh",
            [2] = {
                [1] = {
                    ["Part"] = part,
                    ["Scale"] = size
                }
            }
        }
        _(args)
    end
    
    F3XFunctions.SetLocked = function(part, boolean)
        local args = {
            [1] = "SetLocked",
            [2] = {
                [1] = part
            },
            [3] = boolean
        }
        _(args)
    end
    
    F3XFunctions.SetTrans = function(part, int)
        local args = {
            [1] = "SyncMaterial",
            [2] = {
                [1] = {
                    ["Part"] = part,
                    ["Transparency"] = int
                }
            }
        }
        _(args)
    end
    
    F3XFunctions.Color = function(part, color)
        local args = {
            [1] = "SyncColor",
            [2] = {
                [1] = {
                    ["Part"] = part,
                    ["Color"] = color,
                    ["UnionColoring"] = false
                }
            }
        }
        _(args)
    end
    
    F3XFunctions.SpawnDecal = function(part, side)
        local args = {
            [1] = "CreateTextures",
            [2] = {
                [1] = {
                    ["Part"] = part,
                    ["Face"] = side,
                    ["TextureType"] = "Decal"
                }
            }
        }
        _(args)
    end
    
    F3XFunctions.AddDecal = function(part, asset, side)
        local args = {
            [1] = "SyncTexture",
            [2] = {
                [1] = {
                    ["Part"] = part,
                    ["Face"] = side,
                    ["TextureType"] = "Decal",
                    ["Texture"] = "rbxassetid://" .. asset
                }
            }
        }
        _(args)
    end
    
    F3XFunctions.AddFire = function(part)
        local args = {
            [1] = "CreateDecorations",
            [2] = {
                [1] = {
                    ["Part"] = part,
                    ["DecorationType"] = "Fire"
                }
            }
        }
        _(args)
    end
    
    F3XFunctions.AddSparkles = function(part)
        local args = {
            [1] = "CreateDecorations",
            [2] = {
                [1] = {
                    ["Part"] = part,
                    ["DecorationType"] = "Sparkles"
                }
            }
        }
        _(args)
    end
    
    F3XFunctions.CreateSpotlight = function(part)
        local args = {
            [1] = "CreateLights",
            [2] = {
                [1] = {
                    ["Part"] = part,
                    ["LightType"] = "SpotLight"
                }
            }
        }
        _(args)
    end
    
    F3XFunctions.SyncLighting = function(part, brightness)
        local args = {
            [1] = "SyncLighting",
            [2] = {
                [1] = {
                    ["Part"] = part,
                    ["LightType"] = "SpotLight",
                    ["Brightness"] = brightness
                }
            }
        }
        _(args)
    end
    
    return F3XFunctions
end

