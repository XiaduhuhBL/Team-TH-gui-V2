-- Continue with Part 4 for skyboxes and advanced functions...
-- ========== SKYBOXES AND ADVANCED FUNCTIONS ==========

-- SkyBox Function (Shared)
local function createSkybox(skyId)
    local success, errorMsg = pcall(function()
        local f3x = setupF3XFunctions()
        if f3x.error then
            warn(f3x.error)
            return
        end
        
        local player = game.Players.LocalPlayer
        local char = player.Character
        if not char then return end
        
        local e = char.HumanoidRootPart.CFrame.x
        local f = char.HumanoidRootPart.CFrame.y
        local g = char.HumanoidRootPart.CFrame.z
        
        f3x.CreatePart(CFrame.new(math.floor(e), math.floor(f), math.floor(g)) + Vector3.new(0,6,0), workspace)
        
        for i,v in workspace:GetDescendants() do
            if v:IsA("BasePart") and v.CFrame.x == math.floor(e) and v.CFrame.z == math.floor(g) then
                f3x.SetName(v, "Sky")
                f3x.AddMesh(v)
                f3x.SetMesh(v, "111891702759441")
                f3x.SetTexture(v, skyId)
                f3x.MeshResize(v, Vector3.new(7000, 7000, 7000))
                f3x.SetLocked(v, true)
            end
        end
    end)
    
    if not success then
        warn("Error creating skybox: " .. errorMsg)
    end
end

