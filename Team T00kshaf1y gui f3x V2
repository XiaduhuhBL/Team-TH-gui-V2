-- Create all buttons
local buttonInstances = {}
for i, buttonInfo in ipairs(buttons) do
	buttonInstances[buttonInfo.name] = CreateButton(buttonInfo.name, buttonInfo.text)
end

-- Scripts:

-- Dragging Script
local function DraggingScript()
	local UserInputService = game:GetService("UserInputService")
	local gui = MainFrame
	local dragging
	local dragInput
	local dragStart
	local startPos

	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end

	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end

-- HD Admin Command Function
local function HDAdminCommand(command)
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local RequestCommand = ReplicatedStorage:WaitForChild("HDAdminHDClient").Signals.RequestCommandSilent
	RequestCommand:InvokeServer(command)
end

-- F3X Functions
local function GetF3XTool()
	local player = game.Players.LocalPlayer
	local char = player.Character
	local backpack = player.Backpack
	local tool
	
	for _, v in ipairs(backpack:GetChildren()) do
		if v:FindFirstChild("SyncAPI") then
			tool = v
			break
		end
	end
	
	if not tool then
		for _, v in ipairs(char:GetChildren()) do
			if v:FindFirstChild("SyncAPI") then
				tool = v
				break
			end
		end
	end
	
	return tool
end

local function ExecuteF3XCommand(args)
	local tool = GetF3XTool()
	if tool then
		local remote = tool.SyncAPI.ServerEndpoint
		remote:InvokeServer(unpack(args))
	end
end

-- F3X Utility Functions (from original scripts)
local function SetCollision(part,boolean)
	local args = {
		[1] = "SyncCollision",
		[2] = {
			[1] = {
				["Part"] = part,
				["CanCollide"] = boolean
			}
		}
	}
	ExecuteF3XCommand(args)
end

local function SetAnchor(boolean,part)
	local args = {
		[1] = "SyncAnchor",
		[2] = {
			[1] = {
				["Part"] = part,
				["Anchored"] = boolean
			}
		}
	}
	ExecuteF3XCommand(args)
end

local function CreatePart(cf,parent,partType)
	local args = {
		[1] = "CreatePart",
		[2] = partType or "Normal",
		[3] = cf,
		[4] = parent
	}
	ExecuteF3XCommand(args)
end

local function DestroyPart(part)
	local args = {
		[1] = "Remove",
		[2] = {
			[1] = part
		}
	}
	ExecuteF3XCommand(args)
end

local function MovePart(part,cf)
	local args = {
		[1] = "SyncMove",
		[2] = {
			[1] = {
				["Part"] = part,
				["CFrame"] = cf
			}
		}
	}
	ExecuteF3XCommand(args)
end

local function Resize(part,size,cf)
	local args = {
		[1] = "SyncResize",
		[2] = {
			[1] = {
				["Part"] = part,
				["CFrame"] = cf,
				["Size"] = size
			}
		}
	}
	ExecuteF3XCommand(args)
end

local function AddMesh(part)
	local args = {
		[1] = "CreateMeshes",
		[2] = {
			[1] = {
				["Part"] = part
			}
		}
	}
	ExecuteF3XCommand(args)
end

local function SetMesh(part,meshid)
	local args = {
		[1] = "SyncMesh",
		[2] = {
			[1] = {
				["Part"] = part,
				["MeshId"] = "rbxassetid://"..meshid
			}
		}
	}
	ExecuteF3XCommand(args)
end

local function SetTexture(part, texid)
	local args = {
		[1] = "SyncMesh",
		[2] = {
			[1] = {
				["Part"] = part,
				["TextureId"] = "rbxassetid://"..texid
			}
		}
	}
	ExecuteF3XCommand(args)
end

local function SetName(part, stringg)
	local args = {
		[1] = "SetName",
		[2] = {
			[1] = part
		},
		[3] = stringg
	}
	ExecuteF3XCommand(args)
end

local function MeshResize(part,size)
	local args = {
		[1] = "SyncMesh",
		[2] = {
			[1] = {
				["Part"] = part,
				["Scale"] = size
			}
		}
	}
	ExecuteF3XCommand(args)
end

local function SetLocked(part,boolean)
	local args = {
		[1] = "SetLocked",
		[2] = {
			[1] = part
		},
		[3] = boolean
	}
	ExecuteF3XCommand(args)
end

local function SetTrans(part,int)
	local args = {
		[1] = "SyncMaterial",
		[2] = {
			[1] = {
				["Part"] = part,
				["Transparency"] = int
			}
		}
	}
	ExecuteF3XCommand(args)
end

local function Color(part,color)
	local args = {
		[1] = "SyncColor",
		[2] = {
			[1] = {
				["Part"] = part,
				["Color"] = color,
				["UnionColoring"] = false
			}
		}
	}
	ExecuteF3XCommand(args)
end

local function SpawnDecal(part,side)
	local args = {
		[1] = "CreateTextures",
		[2] = {
			[1] = {
				["Part"] = part,
				["Face"] = side,
				["TextureType"] = "Decal"
			}
		}
	}
	ExecuteF3XCommand(args)
end

local function AddDecal(part,asset,side)
	local args = {
		[1] = "SyncTexture",
		[2] = {
			[1] = {
				["Part"] = part,
				["Face"] = side,
				["TextureType"] = "Decal",
				["Texture"] = "rbxassetid://".. asset
			}
		}
	}
	ExecuteF3XCommand(args)
end

local function AddFire(part)
	local args = {
		[1] = "CreateDecorations",
		[2] = {
			[1] = {
				["Part"] = part,
				["DecorationType"] = "Fire"
			}
		}
	}
	ExecuteF3XCommand(args)
end

-- Skybox function
local function Sky(id)
	local player = game.Players.LocalPlayer
	local char = player.Character
	local e = math.floor(char.HumanoidRootPart.CFrame.x)
	local f = math.floor(char.HumanoidRootPart.CFrame.y)
	local g = math.floor(char.HumanoidRootPart.CFrame.z)
	
	CreatePart(CFrame.new(math.floor(e),math.floor(f),math.floor(g)) + Vector3.new(0,6,0),workspace)
	
	for i,v in game.Workspace:GetDescendants() do
		if v:IsA("BasePart") and v.CFrame.x == math.floor(e) and v.CFrame.z == math.floor(g) then
			SetName(v,"Sky")
			AddMesh(v)
			SetMesh(v,"111891702759441")
			SetTexture(v,id)
			MeshResize(v,Vector3.new(99999,99999,99999))
			SetLocked(v,true)
		end
	end
end

-- Decal Spam function
local function DecalSpam(id)
	for i,v in game.workspace:GetDescendants() do
		if v:IsA("BasePart") then
			spawn(function()
				SetLocked(v,false)
				SpawnDecal(v,Enum.NormalId.Front)
				AddDecal(v,id,Enum.NormalId.Front)

				SpawnDecal(v,Enum.NormalId.Back)
				AddDecal(v,id,Enum.NormalId.Back)

				SpawnDecal(v,Enum.NormalId.Right)
				AddDecal(v,id,Enum.NormalId.Right)

				SpawnDecal(v,Enum.NormalId.Left)
				AddDecal(v,id,Enum.NormalId.Left)

				SpawnDecal(v,Enum.NormalId.Bottom)
				AddDecal(v,id,Enum.NormalId.Bottom)

				SpawnDecal(v,Enum.NormalId.Top)
				AddDecal(v,id,Enum.NormalId.Top)
			end)
		end
	end 
end

-- Fire All function
local function FireAll()
	for i,v in game.Workspace:GetDescendants() do
		spawn(function()
			SetLocked(v,false)
			AddFire(v)
		end)
	end
end

-- Unanchor All function
local function UnanchorAll()
	for i,v in game.Workspace:GetDescendants() do
		spawn(function()
			SetLocked(v,false)
			SetAnchor(false,v)
		end)
	end
end

-- Color Spam function
local function ColorSpam()
	for i,v in game.Workspace:GetDescendants() do
		if v:IsA("BasePart") then
			spawn(function()
				SetLocked(v,false)
				Color(v,Color3.new(math.random(0,255)/255,math.random(0,255)/255,math.random(0,255)/255))
			end)
		end
	end
end

-- Destroy All function
local function DestroyAll()
	for i,v in game.Players:GetPlayers() do
		pcall(function()
			if v.Character then
				DestroyPart(v.Character)
			end
		end)
	end
	
	-- Also destroy workspace parts
	for i,v in game.Workspace:GetDescendants() do
		if v:IsA("BasePart") or v:IsA("MeshPart") then
			spawn(function()
				DestroyPart(v)
			end)
		end
	end
end

