-- Thomas Button (Become thomas)
thomas.Name = "thomas"
thomas.Parent = TextLabel_4
thomas.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
thomas.BorderColor3 = Color3.fromRGB(0, 53, 11)
thomas.Position = UDim2.new(-0.241176412, 0, -29.3571453, 0)
thomas.Size = UDim2.new(0, 99, 0, 39)
thomas.Font = Enum.Font.SourceSans
thomas.Text = "Become thomas (R6)"
thomas.TextColor3 = Color3.fromRGB(255, 255, 255)
thomas.TextScaled = true
thomas.TextSize = 14.000
thomas.TextWrapped = true
thomas.MouseButton1Click:Connect(function()
    local success, errorMsg = pcall(function()
        local f3x = setupF3XFunctions()
        if f3x.error then
            warn(f3x.error)
            return
        end
        
        local player = game.Players.LocalPlayer
        local char = player.Character
        
        -- Make character parts transparent
        local function xd()
            local args = {
                [1] = "SyncMaterial",
                [2] = {
                    [1] = {
                        ["Part"] = char.Torso,
                        ["Transparency"] = 1
                    },
                    [2] = {
                        ["Part"] = char:FindFirstChild("Left Leg"),
                        ["Transparency"] = 1
                    },
                    [3] = {
                        ["Part"] = char.HumanoidRootPart,
                        ["Transparency"] = 1
                    },
                    [4] = {
                        ["Part"] = char:FindFirstChild("Right Leg"),
                        ["Transparency"] = 1
                    },
                    [5] = {
                        ["Part"] = char:FindFirstChild("Right Arm"),
                        ["Transparency"] = 1
                    },
                    [6] = {
                        ["Part"] = char:FindFirstChild("Left Arm"),
                        ["Transparency"] = 1
                    },
                    [7] = {
                        ["Part"] = char.Head,
                        ["Transparency"] = 1
                    },
                }
            }
            f3x.remote:InvokeServer(unpack(args))
        end

        -- Thomas transformation
        local function Thomas(player)
            f3x.SetAnchor(true, char.HumanoidRootPart)
            f3x.CreatePart(char.HumanoidRootPart.CFrame, char)
            
            for i,v in char:GetDescendants() do
                if v.Name == "Part" and v.Parent == char then
                    f3x.SetCollision(v, false)
                    f3x.SetLocked(v, false)
                    f3x.Resize(v, Vector3.new(5,5,10), char.HumanoidRootPart.CFrame)
                    f3x.CreateSpotlight(v)
                    f3x.SyncLighting(v, 100)
                    f3x.SetLocked(char.HumanoidRootPart, false)
                    
                    -- Weld parts (simplified)
                    local args = {
                        [1] = "CreateWelds",
                        [2] = {
                            [1] = v,
                            [2] = char.HumanoidRootPart
                        },
                        [3] = v
                    }
                    f3x.remote:InvokeServer(unpack(args))
                    
                    f3x.SetAnchor(false, v)
                    f3x.AddMesh(v)
                    f3x.MeshResize(v, Vector3.new(2,2,1.5))
                    f3x.SetMesh(v, "2231280549")
                    f3x.SetTexture(v, "2231280614")
                    f3x.SetAnchor(false, char.HumanoidRootPart)
                    char.Humanoid.WalkSpeed = 80
                    
                    -- Sound effects
                    local function SFX(id) 
                        local s = Instance.new("Sound", char.Torso)
                        s.SoundId = "rbxassetid://"..id
                        s.Volume = 1
                        return s
                    end
                    
                    v.Touched:Connect(function(p)
                        if p.Parent and p.Parent:IsA("Model") then
                            if game.Players:FindFirstChild(p.Parent.Name) then
                                if p.Parent.Name ~= player.Name then
                                    spawn(function()
                                        f3x.DestroyPart(game.Players:FindFirstChild(p.Parent.Name).Character.Head)
                                    end)
                                    local Whistle = SFX(475073913)
                                    Whistle:Play()
                                end
                            end
                        end
                    end)

                    local Music = SFX(0)
                    Music.Pitch = 1.15
                    Music:Play()
                end
            end
            
            -- Collision handling
            game:GetService("RunService").RenderStepped:Connect(function()
                for i,v in char:GetDescendants() do
                    if v:IsA("BasePart") then
                        char.Humanoid.WalkSpeed = 70
                        v.CanCollide = false
                    end
                end
            end)
        end
        
        Thomas(player)
        xd()
    end)
    
    if not success then
        warn("Error in Become thomas: " .. errorMsg)
    end
end)

