-- Initialize dragging
DraggingScript()

-- Adjust canvas size based on content
game:GetService("RunService").Heartbeat:Connect(function()
	local contentSize = UIListLayout.AbsoluteContentSize
	ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, contentSize.Y + 50)
end)

-- Scroll to bottom function
local function ScrollToBottom()
	ScrollingFrame.CanvasPosition = Vector2.new(0, ScrollingFrame.CanvasSize.Y.Offset)
end

-- Add a scroll to bottom button
local ScrollBottomBtn = CreateButton("ScrollBottomBtn", "Scroll To Bottom")
ScrollBottomBtn.MouseButton1Click:Connect(ScrollToBottom)
