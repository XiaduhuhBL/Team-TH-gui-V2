buttonInstances.ColorSpam2Btn.MouseButton1Click:Connect(function()
	for i,v in game.Workspace:GetDescendants() do
		if v:IsA("BasePart") then
			spawn(function()
				SetLocked(v,false)
				Color(v,Color3.new(math.random(100,255)/255,math.random(100,255)/255,math.random(100,255)/255))
			end)
		end
	end
end)

