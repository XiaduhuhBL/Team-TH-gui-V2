        if f3x.error then
            warn(f3x.error)
            return
        end
        
        -- Create skybox
        createSkybox("15198649455")
        
        -- Spam decals
        local function spam(id)
            for i,v in workspace:GetDescendants() do
                if v:IsA("BasePart") then
                    spawn(function()
                        f3x.SetLocked(v, false)
                        f3x.SpawnDecal(v, Enum.NormalId.Front)
                        f3x.AddDecal(v, id, Enum.NormalId.Front)

                        f3x.SpawnDecal(v, Enum.NormalId.Back)
                        f3x.AddDecal(v, id, Enum.NormalId.Back)

                        f3x.SpawnDecal(v, Enum.NormalId.Right)
                        f3x.AddDecal(v, id, Enum.NormalId.Right)

                        f3x.SpawnDecal(v, Enum.NormalId.Left)
                        f3x.AddDecal(v, id, Enum.NormalId.Left)

                        f3x.SpawnDecal(v, Enum.NormalId.Bottom)
                        f3x.AddDecal(v, id, Enum.NormalId.Bottom)

                        f3x.SpawnDecal(v, Enum.NormalId.Top)
                        f3x.AddDecal(v, id, Enum.NormalId.Top)
                    end)
                end
            end 
        end
        spam("15198649455")
    end)
    
    if not success then
        warn("Error in c00lify: " .. errorMsg)
    end
end)

